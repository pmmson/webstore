@model  IEnumerable<WebStore.Models.EmployeeView>
@{
    ViewBag.Title = "Сотрудники";
}

<h3>@ViewBag.Title</h3>

<table class="table">
    <thead>
        <tr>
            <th>Фамилия</th>
            <th>Имя</th>
            <th>Отчество</th>
            <th>Возраст</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.ActionLink(@item.SurName, "Details", new { id = item.Id })</td>
        <td>@item.FirstName</td>
        <td>@item.Patronymic</td>
        <td>@item.Age</td>
    </tr>
    }
</table>